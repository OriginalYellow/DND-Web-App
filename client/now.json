{
  "version": 2,
  "name": "dnd-web-app-client",
  "builds": [
      { "src": "package.json", "use": "@now/static-build" }
  ],
  "routes": [
    {"src": "^/js/(.*)", "dest": "/js/$1"},
    {"src": "^/css/(.*)", "dest": "/css/$1"},
    {"src": "^/img/(.*)", "dest": "/img/$1"},
    {"src": "^/fonts/(.*)", "dest": "/fonts/$1"},
    {"src": ".*", "dest": "/index.html"}
  ],
  "alias": "dnd-web-app-client"
}